                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>Select L.Legajos,  E.RazonSocial, Co.Codigo ,D.* 
from 
	(
	     Select isnull(L.Legajos , 0) as Legajos,  Ca.IdCabeceraHojasDeRuta
	     from 
	       CabeceraHojasDeRuta Ca left join 
		(
			Select IdCabecerHojaRuta , Count(IdCabecerHojaRuta) Legajos from ContEmpLegajos 
			group by IdCabecerHojaRuta 
		) as L on L.IdCabecerHojaRuta = Ca.IdCabeceraHojasDeRuta
	) L,

     ContratoEmpresas C 
	inner join Empresa E on E.IdEmpresa = C.IdEmpresa
	inner join Contrato Co on Co.IdContrato = C.IdContrato
	inner join 
	(
	Select * , 'No Aprobadas' EstadoGeneral from
		 (
			SELECT 
				distinct
				H.IdCabeceraHojaDeRuta,
				'Con Presentaciones' as EstadoInterno,
				C.Periodo,
				C.IdContratoEmpresa
			FROM   
				HojasDeRuta H inner join CabeceraHojasDeRuta C on C.IdCabeceraHojasDeRuta = H.IdCabeceraHojaDeRuta
			WHERE  
				C.IdEstado = 15 and
				DocFechaEntrega IS NOT NULL AND
				(C.Periodo &gt;= @FechaInicialPeriodo and C.Periodo &lt;= @FechaFinalPeriodo) and
				DocFechaEntrega &gt;= @FechaInicial and DocFechaEntrega &lt;= @FechaFinal
			GROUP BY H.IdCabeceraHojaDeRuta, C.Periodo,C.IdContratoEmpresa
			
			union
			
			-- Hojas de Ruta sin Documentacion Presentada y no aprobadas
			SELECT    Distinct 
                                                                CabeceraHojasDeRuta.IdCabeceraHojasDeRuta ,
				'Sin Presentaciones' as EstadoInterno,
				CabeceraHojasDeRuta.Periodo,
				CabeceraHojasDeRuta.IdContratoEmpresa
			FROM       CabeceraHojasDeRuta INNER JOIN
			                      HojasDeRuta ON HojasDeRuta.IdCabeceraHojaDeRuta = CabeceraHojasDeRuta.IdCabeceraHojasDeRuta
			WHERE     CabeceraHojasDeRuta.IdEstado = 15 AND 
                                                                 (CabeceraHojasDeRuta.Periodo &gt;= @FechaInicialPeriodo and CabeceraHojasDeRuta.Periodo &lt;= @FechaFinalPeriodo) and
                                                                 HojasDeRuta.IdCabeceraHojaDeRuta NOT IN
			                          (SELECT     IdCabeceraHojaDeRuta
			                            FROM          HojasDeRuta AS HojasDeRuta_1
			                            WHERE      (DocFechaEntrega IS NOT NULL) and
								(Periodo &gt;= @FechaInicialPeriodo and Periodo &lt;= @FechaFinalPeriodo) and
								DocFechaEntrega &gt;= @FechaInicial	 and DocFechaEntrega &lt;= @FechaFinal	
			                            GROUP BY IdCabeceraHojaDeRuta)
			
			Group by CabeceraHojasDeRuta.IdCabeceraHojasDeRuta, CabeceraHojasDeRuta.Periodo,CabeceraHojasDeRuta.IdContratoEmpresa
		) HojasNoAprobadas

	union
	
	Select *, 'Aprobadas' EstadoGeneral from 
		(
			SELECT  CabeceraHojasDeRuta.IdCabeceraHojasDeRuta ,
				'En Termino' as EstadoInterno,
				CabeceraHojasDeRuta.Periodo,
				CabeceraHojasDeRuta.IdContratoEmpresa
			FROM    CabeceraHojasDeRuta 
			WHERE   CabeceraHojasDeRuta.IdEstado = 16 AND 
				(FechaAprobacion IS NOT NULL) AND
				(Periodo &gt;= @FechaInicialPeriodo and Periodo &lt;= @FechaFinalPeriodo) and
				FechaAprobacion &gt;= @FechaInicial	 and FechaAprobacion &lt;= @FechaFinal
			Group by CabeceraHojasDeRuta.IdCabeceraHojasDeRuta, CabeceraHojasDeRuta.Periodo,CabeceraHojasDeRuta.IdContratoEmpresa
	
			union
	
			SELECT  CabeceraHojasDeRuta.IdCabeceraHojasDeRuta ,
				'Fuera Termino' as EstadoInterno,
				CabeceraHojasDeRuta.Periodo,
				CabeceraHojasDeRuta.IdContratoEmpresa
			FROM    CabeceraHojasDeRuta 
			WHERE   CabeceraHojasDeRuta.IdEstado = 16 AND 
				FechaAprobacion IS NOT NULL AND
				(Periodo &gt;= @FechaInicialPeriodo and Periodo &lt;= @FechaFinalPeriodo) and
				FechaAprobacion &gt; @FechaFinal
			Group by CabeceraHojasDeRuta.IdCabeceraHojasDeRuta, CabeceraHojasDeRuta.Periodo,CabeceraHojasDeRuta.IdContratoEmpresa
		) HojasAprobadas
	) D on D.IdContratoEmpresa = C.IdContratoEmpresas
where L.IdCabeceraHojasDeRuta = D.IdCabeceraHojaDeRuta</CommandText>
