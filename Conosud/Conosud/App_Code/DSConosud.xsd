			GROUP BY H.IdCabeceraHojaDeRuta, C.Periodo, cast(month(DocFechaEntrega) as varchar)+'/'+cast(year(DocFechaEntrega) as varchar) ,C.IdContratoEmpresa
			
			union
			
			-- Hojas de Ruta sin Documentacion Presentada y no aprobadas
