          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="GestionAuditoriaTableAdapter" GeneratorDataComponentClassName="GestionAuditoriaTableAdapter" Name="GestionAuditoria" UserDataComponentName="GestionAuditoriaTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ConosudConnectionString (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetDataMesCuestion" GetMethodModifier="Public" GetMethodName="GetDataMesCuestion" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetDataMesCuestion" UserSourceName="GetDataMesCuestion">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>	SELECT 
		distinct
		H.IdCabeceraHojaDeRuta,
		COUNT(DISTINCT H.IdCabeceraHojaDeRuta) CantidadHojas,
		Ct.Nombre,
		Count(DISTINCT Ce.IdContEmpLegajos) as 'CantidadLegajos',
		Count(H.HojaFechaControlado) as 'ItemsControlados',
		(COUNT(DISTINCT H.IdCabeceraHojaDeRuta) * Ti.TotalItem) as 'TotalItems',
		(case when H.AuditadoPor = '' then 'Sin Auditor' else U.UserName end) as 'Auditor'
	FROM   
		HojasDeRuta H 
			inner join Plantilla P on P.IdPlantilla = H.IdPlanilla
			inner join CabeceraHojasdeRuta Ca on Ca.IdCabeceraHojasDeRuta = H.IdCabeceraHojaDeRuta
inner join CategoriasItems Ct on Ct.IdCategoria = P.IdCategoria
			left join aspnet_Users U on cast(U.UserId as varchar(100)) = H.AuditadoPor,
		HojasDeRuta H1 inner join ContEmpLegajos Ce on Ce.IdCabecerHojaRuta = H1.IdCabeceraHojaDeRuta,
		(Select P.IdCategoria , Count(P.IdPlantilla) TotalItem FROM   Plantilla P group by P.IdCategoria ) as Ti
		
	WHERE  
		H.IdHojaDeRuta = H1.IdHojaDeRuta and
		H.DocFechaEntrega IS NOT NULL AND
		((H.HojaFechaControlado &gt;= @FechaInicialConsulta and H.HojaFechaControlado &lt;= @FechaFinalConsulta) or (H.HojaFechaControlado is null)) and
		H.DocFechaEntrega &gt;= @FechaInicialConsulta and H.DocFechaEntrega &lt;= @FechaFinalConsulta and 
		Ca.Periodo &gt;= @FechaInicialPeriodo and Ca.Periodo &lt;= @FechaFinalPeriodo and 
		P.IdCategoria = Ti.IdCategoria
	group by H.IdCabeceraHojaDeRuta,Ct.Nombre,H.AuditadoPor,U.UserName,Ti.TotalItem
</CommandText>
